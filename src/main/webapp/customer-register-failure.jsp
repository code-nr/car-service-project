<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dynamic JSP Page</title>
  <style>
    /* Reset and basic styles */
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 0;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      height: 100vh;
      background-color: #f4f4f9;
    }

    h1 {
      font-size: 24px;
      color: #333;
    }

    .content {
      padding: 20px;
      text-align: center;
      background: #fff;
      border-radius: 8px;
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
      max-width: 600px;
      width: 100%;
    }

    .content p {
      font-size: 16px;
      color: #555;
    }

    /* Responsive design */
    @media (max-width: 480px) {
      h1 {
        font-size: 20px;
      }

      .content p {
        font-size: 14px;
      }
    }
  </style>
</head>
<body>
  <h1> Failed to Register</h1>
  
</body>
</html>
